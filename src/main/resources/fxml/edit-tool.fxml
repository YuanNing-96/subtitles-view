<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1"
          maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="100.0" prefWidth="300.0"
          fx:id="root" fx:controller="org.fordes.subtitles.view.controller.EditTool" style="-fx-background-color: red">
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS"/>
    </rowConstraints>
    <columnConstraints>
        <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <Label fx:id="testLab" text="To be continue" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS"
           GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
        <font>
            <Font size="20.0"/>
        </font>
    </Label>
    <GridPane.margin>
        <Insets top="10.0" right="10.0"/>
    </GridPane.margin>
</GridPane>
